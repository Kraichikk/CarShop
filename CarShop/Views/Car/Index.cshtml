@model List<Car>

<h1>Car List</h1>

<p>
    <a asp-action="Create">Add New Car</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>FullName</th>
            <th>Make</th>
            <th>Model</th>
            <th>Year</th>
            <th>Price</th>
            <th>Odometer</th>
        @*    <th>Transmission</th>
            <th>EngineType</th>
            <th>Condition</th>
            <th>Interior</th>
            <th>WheelDrive</th>*@
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var car in Model)
        {
            <tr>
                <td>@car.Id</td>
                <td>@car.FullName</td>
                <td>@car.Make.Name</td>
                <td>@car.Model.Name</td>
                <td>@car.Year</td>
                <td>@car.Price</td>
                <td>@car.Odometer</td>
                <td>@car.Transmission</td>
                <td>@car.EngineType</td>
                <td>@car.Condition</td>
                <td>@car.InteriorOfCar</td>
                <td>@car.WheelDriwe</td>
                <td>
                    <a asp-action="Delete" asp-route-id="@car.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>